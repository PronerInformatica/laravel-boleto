{
    "name": "proner/laravel-boleto",
    "description": "Biblioteca com boletos para o laravel",
    "keywords": [
        "laravel-boleto"
    ],
    "homepage": "https://github.com/PronerInformatica/laravel-boleto",
    "license": "MIT",
    "authors": [
        {
            "name": "Diogo Bemfica",
            "email": "diogo@proner.com.br"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "nesbot/carbon": "1.*",
        "illuminate/contracts": "~5.1",
        "illuminate/support": "~5.1",
        "illuminate/view": "~5.1",
        "illuminate/events": "~5.1",
        "neitanod/forceutf8": "^2.0",
        "setasign/fpdf": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0.0",
        "orchestra/testbench": "3.2"
    },
    "autoload": {
        "psr-4": {
            "Proner\\LaravelBoleto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Proner\\LaravelBoleto\\Tests\\": "tests"
        }
    }
}
